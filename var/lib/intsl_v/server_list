#!/usr/bin/env bash
if [[ -z $serverstartlist ]];then
    echo "管理するサーバー名を入力してください"
    read -p ">" serverstartlist
fi
case $serverstartlist in
#STARTSERVERLIST





#ENDSERVERLIST
*)
    #不正なキー入力
    echo "存在しないサーバーです"
    ;;
esac
export mc_settings_file_locations
${LIB_FILE_LOCATIONS}mc_server_run/server_default_manager